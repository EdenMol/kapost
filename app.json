{
  "name": "",
  "stack": "heroku-18",
  "scripts": {
    "postdeploy": "rails db:schema:load"
  },
  "env": {
    "NPM_CONFIG_PRODUCTION": "false"
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
    {
      "plan": "ably:bootstrap"
    },
    {
      "plan": "heroku-postgresql:hobby-dev",
      "options": {
        "version": "10"
      }
    }
  ],
  "buildpacks": [
    { "url": "heroku/nodejs" },
    { "url": "heroku/ruby" }
  ]
}
